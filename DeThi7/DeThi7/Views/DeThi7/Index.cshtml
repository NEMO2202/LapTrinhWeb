
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/DeThi7Layout.cshtml";
}
@Html.Partial("_Main")

<script>
   
    $(".itemtem").click(function () {
        var maloai = $(this).attr("id");
        $.ajax({
            url: "RenderProductByMenu",
            data: { maloai: maloai },
            success: function (res) {
                $("div#content").html(res);
            }
        });
    });
</script>
@model IEnumerable<LayoutFootWear_master.Models.Entities.PhanLoai>

<div class="menu text-center">
    <p>
        <a href="#">New Arrivals</a>
        @foreach (var item in Model)
        {
            <a href="#" id="@item.MaPhanLoai" class="item">@item.PhanLoaiChinh</a>
        }
    </p>
</div>
<script>

    $(".item").click(function () {
        var maloai = $(this).attr("id");
        $.ajax({
            url: "RenderProductByMenu",
            data: { maloai: maloai },
            success: function (res) {
                $("div#content").html(res);
            }
        });
    });
</script>
